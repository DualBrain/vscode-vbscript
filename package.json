{
	"name": "VBScript",
	"displayName": "VBScript",
	"description": "VBScript language syntax highlighting support for Visual Studio Code",
	"version": "0.0.1",
	"publisher": "luggage66",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Languages"
	],
    "repository": {
      "type": "git",
      "url": "https://github.com/luggage66/vscode-vbscript.git" 
    },
	"contributes": {
		"languages": [{
			"id": "vbscript",
			"aliases": ["VBScript", "vbscript"],
			"extensions": [".vbs",".vbe",".wsf",".wsc",".acm",".acr",".acf"],
			"configuration": "./vbscript.configuration.json"
		}],
		"grammars": [{
			"language": "vbscript",
			"scopeName": "source.vbs",
			"path": "./syntaxes/vbscript.json"
		}]
	}
}